{
  "name": "fv1-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky",
    "update_client": "npx @openapitools/openapi-generator-cli generate -g typescript-axios -o src/clients/fv1 --enable-post-process-file --global-property skipFormModel=false --type-mappings DateTime=Date -i ~/Downloads/tmp/docs-json.json && prettier --write src/clients",
    "test": "npm run dev",
    "cypress": "cypress open"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.7",
    "@mui/material": "^5.15.7",
    "@reduxjs/toolkit": "^2.1.0",
    "@types/jwt-encode": "^1.0.3",
    "axios": "^1.6.7",
    "axios-retry": "^4.0.0",
    "formik": "^2.4.5",
    "jwt-decode": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "cypress": "^13.6.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.0.10",
    "jwt-encode": "^1.0.1",
    "lint-staged": "^15.2.1",
    "prettier": "^3.2.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
